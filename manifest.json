{
    "manifest_version": 2,
    "name": "GO 语言中文网阅读小助手",
    "description": "GO 语言中文网推出的阅读小助手，让你不错过任何信息！",
    "version": "1.0",
    "background": {
        "persistent": false,
        "scripts": [ "/js/background.js" ]
    },
    "chrome_url_overrides": {
        "newtab": "html/newtab.html"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img/32.png",
        "default_tirle": "Go中文网分享阅读插件"
    }
}